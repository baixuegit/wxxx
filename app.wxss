/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
}
/* 自定义tarbar样式 */
.tabBar_wapper {
  width: 100%;
  height: 120rpx;
  background-color: #ccc;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1rpx solid #fff;
  z-index: 9999;
  display: flex;
}
.tabbar_nav {
  float: left;
  flex: 1;
  text-align: center;
  /* line-height: 120rpx; */
}
.tabbar_icon {
  margin: 0 auto;
  display: block;
  width: 60rpx;
  height: 60rpx;
}
